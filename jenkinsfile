pipeline{
    agent any
    
    stages{
        
    
     stage('cloning ...'){
        steps{
           git branch: 'main', url: 'https://github.com/devopsPRO27/flaskandjenkins.git'
        }  
        
     }
     
     stage('build'){
         steps{
             sh 'python3 flask3.py'
         }
     }
     stage ('Testing'){
         steps{
             sh 'echo testing'
         }
     }
     
        
    }
}
